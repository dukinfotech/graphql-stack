  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start Frontend",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "runtimeArgs": [
        "dev"
      ],
      "envFile": "${workspaceFolder}/.env",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Backend",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}/backend",
      "outputCapture": "std",
      "runtimeArgs": [
        "start:dev"
      ],
      "envFile": "${workspaceFolder}/.env",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Frontend: Graphql Codegen",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}/frontend",
      "outputCapture": "std",
      "runtimeArgs": [
        "graphql-code-generate"
      ]
    },
  ]
}